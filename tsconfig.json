{
    "files": [
        "./main.ts",
    ],
    "include": [
        "RPCs/*"
    ],
    "compilerOptions": {
        "target": "es5",
        "typeRoots": [
            "./node_modules"
        ],
        "outFile": "./build/index.js",
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipDefaultLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}